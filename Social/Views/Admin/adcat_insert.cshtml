@model Social.Models.adcategory

@{
    ViewBag.Title = "adcat_insert";
    Layout = "~/Adminpanel.cshtml";
}

<hr />
<h4 class="mb-3">Add Ad Category</h4><hr />
<div class="container">
    <div class="" id="nav-addnew" role="tabpanel" aria-labelledby="nav-addnew-tab">
    
        <form method="post" enctype="multipart/form-data">
            <div class="form-group">
                <label for="adcat_name">Category Name</label>
                @Html.EditorFor(model => model.adcat_name, new { htmlAttributes = new { @class = "form-control" }, id = "adcat_name" })
            </div>
         
      
            <div class="form-row">
                <div class="form-group col-md-6">
                    <button type="submit" class="btn btn-dark btn-outline-secondary">Submit</button>
                </div>
            </div>
        </form>





    </div>
</div>